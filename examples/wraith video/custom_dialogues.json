            {
                "setBackground": "purple",
                "icon": 55,
                "name": "The Wraith",
                "sound": "sfx/deltawhip.wav",
                "content": "<cr><s100>EVERYONE SHUT UP!</s></c>"
            },
            {
                "icon": 54,
                "name": "The Wraith",
                "content": "We need to come up with a <cg>plan</c>."
            },
            {
                "setBackground": "brown",
                "customIcon": "portraits/wulzy.png",
                "name": "Wulzy",
                "content": "We could do a <cy>level swap</c>!",
                "textSound": "blips/wulzy.wav"
